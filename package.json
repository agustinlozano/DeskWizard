{
  "name": "desk-wizard",
  "private": true,
  "version": "0.0.6",
  "description": "A MERN app to manage tickets.",
  "workspaces": [
    "frontend",
    "server"
  ],
  "engines": {
    "npm": "8.5.5"
  },
  "main": "index.mjs",
  "scripts": {
    "build": "npm run build --workspace=frontend",
    "start:app": "npm run dev --workspace=frontend",
    "start": "npm run start --workspace=server",
    "deploy": "npm run build && npm run start",
    "dev:server": "npm run build && npm run dev --workspace=server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agustinlozano/DeskWizard.git"
  },
  "keywords": [],
  "author": "Agustin Lozano Blua",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/agustinlozano/DeskWizard/issues"
  },
  "homepage": "https://github.com/agustinlozano/DeskWizard#readme",
  "devDependencies": {
    "standard": "17.0.0"
  }
}
